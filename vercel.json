{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "routes": [
    {
      "src": "/static/(.*)",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "REACT_APP_API_BASE_URL": "https://vcbabackend-production.up.railway.app",
    "REACT_APP_WEBSOCKET_URL": "https://vcbabackend-production.up.railway.app",
    "REACT_APP_NAME": "VCBA E-Bulletin Board",
    "REACT_APP_VERSION": "1.0.0",
    "REACT_APP_ENABLE_NOTIFICATIONS": "true",
    "REACT_APP_ENABLE_DARK_MODE": "true",
    "REACT_APP_ENABLE_DEBUG_LOGS": "false",
    "GENERATE_SOURCEMAP": "false",
    "CI": "false"
  }
}
